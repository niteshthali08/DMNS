/* Program to illustrate factorial function */

func fibonaci(n)
{
    res = 0
    if(n == 0)
    {
        return 0
    }
    if (n == 1)
    {
        return 1
    }
    t = n-1
    fa = fibonaci(t)
    t = n-2
    fb = fibonaci(t)
    res = fa + fb
    return res
}

m = 7
res = fibonaci(m)
print "fibonaci",res

/* 
Input : m = 7
Output : fibonaci 13 
*/